        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            ".underline": {
              "padding-top": ".5rem",
              ".line": {
                "width": "5rem",
                "height": "1px",
                "display": "inline-block",
                "background-color": "@underlineColor",
                "transition": "width .3s"
              }
            },
            ".first-col, .second-col": {
              "display": "flex",
              "-webkit-align-items": "center",
              "align-items": "center",
              "-webkit-justify-content": "center",
              "justify-content": "center"
            },
            ".first-col:hover, .second-col:hover": {
              ".line": {
                "width": "6rem"
              }
            },
            "& when (@showTitle)": {
              ".col-wrap, .first-column-text": {
                "padding-top": "1rem"
              }
            },
            ".mbr-text": {
              "margin": "0"
            },
            "& when (@showSecond)": {
              ".iconfont-wrapper": {
                "display": "inline-block",
                "width": "calc(~\"@{iconsSize}rem + 1rem\")",
                "height": "calc(~\"@{iconsSize}rem + 1rem\")"
              },
              ".icons-list": {
                "padding-top": "1rem",
                "a:not(:last-child)": {
                  "margin-right": ".5rem"
                },
                "span": {
                  "border-radius": "50%",
                  "padding": "0.5rem 0.6rem",
                  "color": "@iconsColor",
                  "border": "1px solid @iconsColor",
                  "transition": "all .3s",
                  "width": "calc(~\"@{iconsSize}rem + 1rem\")",
                  "height": "calc(~\"@{iconsSize}rem + 1rem\")",
                  "font-size": "~\"@{iconsSize}rem\"",
                  "&:hover": {
                    "color": "@iconsHover",
                    "-webkit-transform": "scale(1.2, 1.2)",
                    "transform": "scale(1.2, 1.2)"
                  }
                }
              },
              ".field": {
                "padding": "0"
              },
              ".dragArea": {
                "margin": "0",
                "justify-content": "center"
              },
              ".field-input": {
                "background": "transparent",
                "padding": ".6rem",
                "height": "100%",
                "color": "@inputColor"
              },
              ".mbr-form .mbr-section-btn": {
                "margin": "0",
                ".btn": {
                  "height": "100%",
                  "padding": "1rem"
                }
              }
            },
            "& when (@showThird)": {
              ".map-placeholder": {
                "display": "none"
              },
              ".google-map": {
                "height": "325px",
                "position": "relative",
                "width": "100%",
                "iframe": {
                  "height": "100%",
                  "width": "100%"
                },
                "[data-state-details]": {
                  "color": "#ffffff",
                  "height": "1.5em",
                  "margin-top": "-0.75em",
                  "padding-left": "1.25rem",
                  "padding-right": "1.25rem",
                  "position": "absolute",
                  "text-align": "center",
                  "top": "50%",
                  "width": "100%"
                },
                "&[data-state]": {
                  "background": "#e9e5dc"
                },
                "&[data-state=\"loading\"]": {
                  "[data-state-details]": {
                    "display": "none"
                  }
                }
              }
            },
            "@media (max-width: 991px)": {
              ".md-pb": {
                "padding-bottom": "2rem"
              }
            },
            "@media (max-width: 767px)": {
              ".sm-pb": {
                "padding-bottom": "2rem"
              },
              ".mbr-form .mbr-section-btn": {
                "margin-top": ".5rem"
              }
            }
          },
          "_name": "footer3",
          "_customHTML": "<section class=\"footer3 map\" group=\"Footers\" plugins=\"amp-main, amp-form, amp-mustache, amp-iframe\" global>\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"3\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"3\">\n        \n        <input type=\"checkbox\" title=\"Show Column Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Underline\" name=\"showUnderline\" condition=\"showTitle\" checked>\n        <input type=\"color\" title=\"Underline Color\" name=\"underlineColor\" value=\"#b69e59\" condition=\"showUnderline && showTitle\">\n        <input type=\"checkbox\" title=\"Show 1st Column\" name=\"showFirst\" checked>\n        <input type=\"checkbox\" title=\"Show 2nd Column\" name=\"showSecond\">\n        <input type=\"color\" title=\"Input Text Color\" name=\"inputColor\" value=\"#b69e59\" condition=\"showSecond\">\n        <input type=\"range\" inline title=\"Icons Count\" name=\"iconsCount\" min=\"0\" max=\"6\" step=\"1\" value=\"4\" condition=\"showSecond\">\n        <input type=\"range\" inline title=\"Icons Size\" name=\"iconsSize\" min=\"1\" max=\"4\" step=\"0.1\" value=\"1.5\" condition=\"showSecond &amp;&amp; iconsCount > 0\">\n        <input type=\"color\" title=\"Icons Color\" name=\"iconsColor\" value=\"#b69e59\" condition=\"showSecond &amp;&amp; iconsCount > 0\">\n        <input type=\"color\" title=\"Icons Hover Color\" name=\"iconsHover\" value=\"#ffffff\" condition=\"showSecond &amp;&amp; iconsCount > 0\">\n        <input type=\"checkbox\" title=\"Show 3rd Column\" name=\"showThird\" checked>\n        <input type=\"map\" title=\"Map\" name=\"googleMap\" condition=\"showThird\" place-id=\"ChIJlbHw20zNyUcRWGQsDukmPWg\" value=\"Baan Thai Groningen \nPaterswoldseweg 66 \n9726 BG Groningen\">\n\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background6.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#232323\" selected>\n        </fieldset>\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type!='color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#232323\" condition=\"bg.type!='color' && overlay\">\n        <input type=\"range\" inline title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"bg.type!='color' && overlay\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <div class=\"container\">\n        <div class=\"mbr-row mbr-justify-content-center\">\n            <div class=\"first-col mbr-col-sm-12 mbr-col-md-6 align-center mbr-white\" mbr-class=\"{'sm-pb': showSecond || showThird,\n                             'mbr-col-lg-4 md-pb': showSecond && showThird}\" mbr-if=\"showFirst\">\n                <div class=\"column-wrapper\">\n                    <h3 class=\"mbr-fonts-style group-title\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".group-title, .underline\">\n                        CONTACT</h3>\n                    <div class=\"underline\" mbr-if=\"showUnderline && showTitle\">\n                        <div class=\"line\"></div>\n                    </div>\n                    <p class=\"first-column-text mbr-text mbr-fonts-style mbr-light mbr-white\" mbr-theme-style=\"display-7\" data-app-selector=\".first-column-text\"><b>Baan Thai Groningen\n</b><br>Paterswoldseweg 66 \n<br>9726 BG Groningen<br><br>\n                        <a href=\"tel:+31503148408\">Tel: </a><b><a href=\"tel:+31503148408\">050 - 3148408</a></b><br><a href=\"mailto:info@baanthaigroningen.nl\">info@baanthaigroningen.nl&nbsp;</a><br><br>Omdat alle relevante informatie op onze site staat, verzoeken wij u ons alleen te bellen of te mailen wanneer u een afspraak wilt maken.<br><br>Gemeentelijke Exploitatievergunning nr. 2007 02 139<br></p>\n                </div>\n            </div>\n\n            <div class=\"second-col mbr-col-sm-12 mbr-col-md-6 align-center mbr-white\" mbr-class=\"{'sm-pb md-pb': showThird,\n                             'mbr-col-lg-4': showFirst && showThird}\" mbr-if=\"showSecond\">\n                <div class=\"column-wrapper\">\n                    <h3 class=\"mbr-fonts-style group-title mbr-white\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".group-title, .underline\">\n                        GET OFFERS\n                    </h3>\n                    <div class=\"underline\" mbr-if=\"showUnderline && showTitle\">\n                        <div class=\"line\"></div>\n                    </div>\n                    <div class=\"col-wrap\">\n                        <div mbr-form>\n                            <!---Formbuilder Form--->\n                        <form action action-xhr name=\"Mobirise Form\" method=\"POST\" class=\"mbr-form\">\n                            <div class=\"mbr-row\">\n                                <div submit-success class=\"mbr-col-md-12  mbr-col-sm-12\">\n                                    <template data-form-alert type=\"amp-mustache\">Subscription successful!</template>\n                                </div>\n                                <div submit-error class=\"mbr-col-md-12  mbr-col-sm-12\">\n                                    <template data-form-alert type=\"amp-mustache\">Subscription failed! {{error}}</template>\n                                </div>\n                            </div>\n                            <div class=\"dragArea mbr-row\">\n                                <div data-for=\"form[data][0][1]\" class=\"mbr-col  field\">\n                                    <input type=\"hidden\" name=\"form[data][0][0]\" value=\"Email\">\n                                    <input type=\"email\" name=\"form[data][0][1]\" placeholder=\"Email\" data-form-field=\"Email\" mbr-theme-style=\"display-7\" required=\"required\" class=\"field-input\">\n                                </div>\n                                <div data-for mbr-buttons=\"true\" mbr-theme-style=\"display-4\" data-toolbar=\"-mbrBtnMove,-mbrBtnLink,-mbrBtnAdd\" class=\"mbr-col-auto  mbr-col-sm-12 mbr-section-btn field align-left\">\n                                    <a type=\"submit\" class=\"btn btn-primary-outline\">SUBSCRIBE</a>\n                                </div>\n                            </div>\n                        </form><!---Formbuilder Form--->\n                        </div>\n                        <div class=\"icons-list\" mbr-list mbr-list-grow=\"iconsCount\">\n                            <a href=\"https://www.facebook.com/pages/Mobirise/1616226671953247\" target=\"_blank\">\n                                <div class=\"iconfont-wrapper\">\n                                    <span mbr-icon class=\"amp-iconfont fa-facebook-f fa\"></span>\n                                </div>\n                            </a>\n                            <a href=\"https://twitter.com/mobirise\" target=\"_blank\">\n                                <div class=\"iconfont-wrapper\">\n                                    <span mbr-icon class=\"amp-iconfont fa-twitter fa\"></span>\n                                </div>\n                            </a>\n                            <a href=\"https://www.pinterest.com/mobirise/\" target=\"_blank\">\n                                <div class=\"iconfont-wrapper\">\n                                    <span mbr-icon class=\"amp-iconfont fa-pinterest-p fa\"></span>\n                                </div>\n                            </a>\n                            <a href=\"https://instagram.com/mobirise/\" target=\"_blank\">\n                                <div class=\"iconfont-wrapper\">\n                                    <span mbr-icon class=\"amp-iconfont fa-instagram fa\"></span>\n                                </div>\n                            </a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"third-col mbr-col-sm-12 mbr-col-md-6 last-col align-center\" mbr-class=\"{'mbr-col-lg-4': showFirst && showSecond}\" mbr-if=\"showThird\">\n                <div class=\"google-map\" mbr-map=\"googleMap\"></div>\n                <div class=\"map-placeholder mobirise-loader\">\n                    <div placeholder class=\"placeholder\"></div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
